{
  "name": "kof-parser",
  "version": "0.1.0",
  "description": "Parser for Norwegian KOF files. Works as ES module and via script injection. Supports JavaScript and TypeScript.",
  "main": "dist/kof-parser.cjs.js",
  "module": "dist/kof-parser.esm.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kof-parser.esm.js",
      "require": "./dist/kof-parser.cjs.js",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "node test/cleanup_logs.js && mocha --recursive test/mocha",
    "demo": "node demo/demo.js"
  },
  "keywords": [
    "kof",
    "kof-fil",
    "koordinatfil",
    "norkart",
    "1988",
    "parser",
    "norwegian",
    "file format"
  ],
  "author": "BrUSomania",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrUSomania/kof-parser.git"
  },
  "bugs": {
    "url": "https://github.com/BrUSomania/kof-parser/issues"
  },
  "homepage": "https://github.com/BrUSomania/kof-parser#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^20.0.0",
    "iconv-lite": "^0.7.0",
    "mocha": "^10.2.0",
    "pdf-parse": "^1.1.1",
    "proj4": "^2.8.1",
    "rollup": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  }
}
